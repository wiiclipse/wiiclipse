#!/usr/bin/env python
import os, sys, commands

desc=commands.getoutput('git describe')

pos=desc.find("-")
version=desc[:pos]
qualifier=desc[pos+1:]
input = sys.stdin
output = sys.stdout
for s in input:
	if s.startswith("Bundle-Version:"):
		s="Bundle-Version: " + version + "." + qualifier +"\n"

	output.write(s)
