#!/usr/bin/env python
import os, sys, commands, time

version=commands.getoutput('git describe --abbrev=0')
version=version+"-"
qualifier=commands.getoutput('git describe')
qualifier=qualifier.lstrip(version)

input = sys.stdin
output = sys.stdout
for s in input:
	if s.startswith("qualifier="):
		s="qualifier=" + qualifier + "\n"

	output.write(s)
