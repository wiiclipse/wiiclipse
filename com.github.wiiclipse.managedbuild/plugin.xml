         </builder>
         <targetPlatform
               binaryParser="org.eclipse.cdt.core.ELF"
               id="com.github.wiiclipse.managedbuild.toolchain.platform.base">
         </targetPlatform>
         <tool
               id="com.github.wiiclipse.managedbuild.toolchain.assembler.base"
               isAbstract="false"
               superClass="com.github.wiiclipse.managedbuild.assembler">
         </tool>
         <tool
               id="com.github.wiiclipse.managedbuild.toolchain.c.compiler.base"
               superClass="com.github.wiiclipse.managedbuild.c.compiler">
         </tool>
         <tool
               id="com.github.wiiclipse.managedbuild.toolchain.cpp.compiler.base"
               superClass="com.github.wiiclipse.managedbuild.cpp.compiler">
         </tool>
         <tool
               id="com.github.wiiclipse.managedbuild.toolchain.c.linker.base"
               superClass="com.github.wiiclipse.managedbuild.c.linker">
         </tool>
         <tool
               id="com.github.wiiclipse.managedbuild.toolchain.cpp.linker.base"
               superClass="com.github.wiiclipse.managedbuild.cpp.linker">
         </tool>
         <tool
               id="com.github.wiiclipse.managedbuild.toolchain.elf2dol.base"
               superClass="com.github.wiiclipse.managedbuild.elf2dol">
         </tool>
         <tool
               id="com.github.wiiclipse.managedbuild.toolchain.archiver.base"
               isAbstract="false"
               superClass="com.github.wiiclipse.managedbuild.archiver">
         </tool>
         <supportedProperties>
            <property
                  id="org.eclipse.cdt.build.core.buildArtefactType">
               <value
