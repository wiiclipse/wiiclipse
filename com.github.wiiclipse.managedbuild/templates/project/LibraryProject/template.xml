<?xml version="1.0" encoding="UTF-8"?>
<template type="ProjTempl" version="1.0" supplier="WiiClipse" revision="1.0"
	author="wiiclipse.github.com" id="StaticLibrary" label="Static Library" description="">
	<process type="com.github.wiiclipse.managedbuild.NewProject">
		<simple name="projName" value="$(projectName)" />
		<simple name="projType" value="library" />
	</process>

	<process type="org.eclipse.cdt.core.CreateSourceFolder">
		<simple name="projectName" value="$(projectName)"/>
		<simple name="path" value="source"/>
	</process>
</template>
